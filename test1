#include<stdio.h>
int main()
{
int a[2][2],transp[2][2];
int i,j;
for(i=0;i<2;i++)
{
for (j=0;j<2;j++)
{
printf("enter element at a%d%d:",i,j);
scanf("%d",&a[i][j]);
}
}
for(i=0;i<2;i++)
{
for (j=0;j<2;j++)
{
transp[i][j]=a[j][i];
}
}
printf("transpose");
for(i=0;i<2;i++)
{
for (j=0;j<2;j++)
{
printf("\t%d",transp[i][j]);
}
}

}
